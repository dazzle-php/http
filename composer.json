{
    "name": "dazzle-php/http",
    "description": "Dazzle Asynchronous HTTP Client / Server.",
    "keywords": [
        "dazzle", "dazzle-php", "async", "non-blocking", "http", "socket", "server", "socket-server", "client", "socket-client"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/dazzle-php/http/issues",
        "source": "https://github.com/dazzle-php/http"
    },
    "authors": [
        {
            "name":     "Kamil Jamroz",
            "homepage": "https://github.com/khelle"
        },
        {
            "name":     "The contributors",
            "homepage": "http://github.com/dazzle-php/http/contributors"
        }
    ],
    "require": {
        "php": ">=5.6.7",
        "dazzle-php/socket": "0.5.*",
        "dazzle-php/loop": "0.5.*",
        "dazzle-php/throwable": "0.5.*",
        "dazzle-php/util": "0.5.*",
        "guzzlehttp/guzzle": "^6.1",
        "symfony/http-foundation": "^3.0",
        "symfony/routing": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8.0 <5.4.0"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Http\\": "src/Http",
            "Dazzle\\Http\\Test\\": "test"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.5-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
